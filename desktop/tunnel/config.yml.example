# Cloudflare Tunnel configuration
# Copy to config.yml and fill in your values.
#
# See: https://developers.cloudflare.com/cloudflare-one/connections/connect-networks/configure-tunnels/

tunnel: YOUR_TUNNEL_ID
credentials-file: /home/user/.cloudflared/YOUR_TUNNEL_ID.json

ingress:
  # Symbi HTTP API
  - hostname: agents.yourdomain.com
    service: http://localhost:8081
    originRequest:
      noTLSVerify: false

  # Operations Console (optional)
  # - hostname: console.yourdomain.com
  #   service: http://localhost:3001
  #   originRequest:
  #     noTLSVerify: false

  # Qdrant (optional â€” usually internal only)
  # - hostname: qdrant.yourdomain.com
  #   service: http://localhost:6333

  # Catch-all: return 404 for unmatched routes
  - service: http_status:404
